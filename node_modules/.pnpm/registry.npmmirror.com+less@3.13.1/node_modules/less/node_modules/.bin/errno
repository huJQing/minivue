#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../registry.npmmirror.com+errno@0.1.8/node_modules/errno/cli.js" "$@"
else
  exec node  "$basedir/../../../../../registry.npmmirror.com+errno@0.1.8/node_modules/errno/cli.js" "$@"
fi
