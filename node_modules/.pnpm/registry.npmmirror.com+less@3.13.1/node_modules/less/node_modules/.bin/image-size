#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../registry.npmmirror.com+image-size@0.5.5/node_modules/image-size/bin/image-size.js" "$@"
else
  exec node  "$basedir/../../../../../registry.npmmirror.com+image-size@0.5.5/node_modules/image-size/bin/image-size.js" "$@"
fi
