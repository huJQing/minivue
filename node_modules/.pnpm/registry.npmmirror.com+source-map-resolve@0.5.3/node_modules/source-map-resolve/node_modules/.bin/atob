#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../registry.npmmirror.com+atob@2.1.2/node_modules/atob/bin/atob.js" "$@"
else
  exec node  "$basedir/../../../../../registry.npmmirror.com+atob@2.1.2/node_modules/atob/bin/atob.js" "$@"
fi
